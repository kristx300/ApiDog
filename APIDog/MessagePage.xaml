<Window x:Class="APIDog.MessagePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:APIDog"
        mc:Ignorable="d"
        Title="MessagePage" Height="150" Width="300" WindowStartupLocation="CenterScreen" Icon="pack://application:,,,/Resources/dogI.ico">
    <Grid>
        <Button Content="Okay" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5,5" Width="75" Click="Okay_Click"/>
        <Label x:Name="TitleM" Content="Label" HorizontalAlignment="Left" Margin="220,0,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="Text" HorizontalAlignment="Left" Margin="100,30,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="54" Width="185"/>
        <Image HorizontalAlignment="Left" Height="73" Margin="13,17,0,0" VerticalAlignment="Top" Width="73" Source="pack://application:,,,/Resources/circle.png" />
    </Grid>
</Window>
